:: Widgets Settings [widget]

<<widget "initsettings">><<nobr>>
<<set $player.gender to "f">>
<<set $player.sex to "f">>
<<set $player.gender_body to "a">>
<<set $player.ballsExist to false>>
<<set $player.freckles to false>>
<<set $devlevel to 12>>
<<set $bodysize to 2>>
<<set $breastsize to 0>>
<<set $breastsensitivity to 1>>
<<set $penissize to 1>>
<<set $genitalsensitivity to 1>>
<<set $eyeselect to "purple">>
<<set $hairselect to "red">>
<<set $hairlength to 200>>
<<set $awareselect to "innocent">>
<<set $background to "waif">>
<<if StartConfig.enableImages is true>>
	<<set $images to 1>>
	<<set $silhouettedisable to "f">>
<</if>>
<<set $numberify_enabled to 1>>
<<set $gamemode to "normal">>
<<set $whitechance to 90>>
<<set $blackchance to 10>>
<<set $bestialitydisable to "f">>
<<set $swarmdisable to "f">>
<<set $spiderdisable to "f">>
<<set $slugdisable to "f">>
<<set $waspdisable to "f">>
<<set $bodywritingdisable to "f">>
<<set $parasitedisable to "f">>
<<set $bestialitydisable to "f">>
<<set $slimedisable to "f">>
<<set $voredisable to "f">>
<<set $tentacledisable to "f">>
<<set $analdisable to "f">>
<<set $transformdisable to "f">>
<<set $hirsutedisable to "t">>
<<set $breastfeedingdisable to "f">>
<<set $analpregdisable to "f">>
<<set $asphyxiaLvl to 3>>
<<set $tipdisable to "f">>
<<set $bodywritingImages to true>>
<<if $debug is 1>>
	<<set $debugdisable to "f">>
<<else>>
	<<set $debugdisable to "t">>
<</if>>
	<<set $cheatdisable to "t">>
<<set $watersportsdisable to "f">>
<<set $timestyle to "military">>
<<set $saveName to "">>
<<set $zoom to 100>>
<<set $checkstyle to "words">>
<<set $blackwolfmonster to 1>>
<<set $greathawkmonster to 1>>
<<set $player.ballsExist to false>>
<<set $blinkingdisable to "f">>
<<set $halfcloseddisable to "f">>

<<set $newWardrobeStyle to false>>
<<set $imgLighten to false>>
<<set $sidebarStats to "Disabled">>
<<set $sidebarTime to "Disabled">>
<<set $combatControls to "radio">>
<<set $reducedLineHeight to false>>
<<set $sidebarAnimations to true>>
<<set $combatAnimations to true>>
<<startingPlayerImageReset>>
<<init_customSetting>><</nobr>><</widget>>

<<widget "displaySettings">><<nobr>>
<<if $args[0]>>
	<<set $currentSettings to $args[0]>>
<</if>>
<<if $passage isnot "Start" and $passage isnot "certification_event_start" and ["quickStart", "presetConfirm", "characterSettings", "featSettings"].includes($currentSettings)>>
	<<set $currentSettings to "gameSettings">>
<</if>>
<<switch $currentSettings>>
	<<case "quickStart">><<replace #settingsDiv>><<quickStart>><</replace>>
	<<case "presetConfirmDetails">><<replace #settingsDiv>><<presetConfirmDetails>><</replace>>
	<<case "characterSettings">>
		<<replace #settingsDiv>><<characterSettings>><</replace>>
		<<if StartConfig.enableImages and ($passage is "Start" or $passage is "certification_event_start")>><<startingPlayerImageUpdate>><<removeclass #startingPlayerImage "hidden">><</if>>
	<<case "featSettings">><<replace #settingsDiv>><<featSettings>><</replace>>
	<<case "gameSettings">><<replace #settingsDiv>><<gameSettings>><</replace>>
	<<case "sizeLimitsSettings">><<replace #settingsDiv>><<sizeLimitsSettings>><</replace>>
	<<case "npcSettings">><<replace #settingsDiv>><<npcSettings>><</replace>>
	<<case "generalSettings">><<replace #settingsDiv>><<generalSettings>><</replace>>
	<<case "exportsettings">><<replace #settingsDiv>><<exportsettings>><</replace>>
	<<case "importConfirmDetails">><<replace #settingsDiv>><<importConfirmDetails>><</replace>>
	<<default>><<replace #settingsDiv>><<gameSettings>><</replace>>
<</switch>>
<<if $currentSettings isnot "importConfirmDetails">>
	<<unset $importString>>
<</if>>
<</nobr>><</widget>>

<<widget "displaySubsection">><<nobr>>
<<if $args[0]>>
	<<set $currentSubsection to $args[0]>>
<</if>>
<<switch $currentSubsection>>
	<<case "humanSettings">><<replace #subsectionDiv>><<humanSettings>><</replace>>
	<<case "beastSettings">><<replace #subsectionDiv>><<beastSettings>><</replace>>
	<<default>><<replace #subsectionDiv>><<humanSettings>><</replace>>
<</switch>>
<</nobr>><</widget>>

<<widget "settings">><<nobr>>
<<if $passage is "Start" or $passage is "certification_event_start">>
<div id="settingsExit">
	<<settingsExit>>
</div>
<br>
<</if>>
<div class="gold">
	<<if $passage is "Start" or $passage is "certification_event_start">>
		<<button "빠른 시작<span class='red'>*</span>">>
			<<displaySettings "quickStart">>
		<</button>>
		<<button "캐릭터 생성<span class='red'>*</span>">>
			<<displaySettings "characterSettings">>
		<</button>>
		<<if $images is 1>>
			<<button "도전과제 부스트<span class='red'>*</span> <img src='img/ui/GoldCoin.gif' style='height:1.1em;margin-bottom:-3px;'/>">>
				<<displaySettings "featSettings">>
			<</button>>
		<<else>>
			<<button "도전과제 부스트<span class='red'>*</span>">>
				<<displaySettings "featSettings">>
			<</button>>
		<</if>>
	<</if>>
	<<button "게임 설정">>
		<<displaySettings "gameSettings">>
	<</button>>
	<<button "크기 제한">>
		<<displaySettings "sizeLimitsSettings">>
	<</button>>
	<<button "네임드 NPC 설정">>
		<<displaySettings "npcSettings">>
	<</button>>
	<<button "일반">>
		<<displaySettings "generalSettings">>
	<</button>>
	<<button "설정 내보내기/가져오기">>
		<<displaySettings "exportsettings">>
	<</button>>
</div>
<br>
<div id="settingsDiv" class="no-numberify">
	<<if $passage is "Start" or $passage is "certification_event_start">>
		<<quickStart>><<set $currentSettings to "quickStart">>
	<<else>>
	<<gameSettings>><<set $currentSettings to "gameSettings">>
	<</if>>
</div>
<br>
<<link "페이지 상단으로">>
	<<script>>
		document.body.scrollTop = 0;
		document.documentElement.scrollTop = 0;
	<</script>><!--</script>-->
<</link>>
<<if $passage isnot "Start" and $passage isnot "certification_event_start">>
<br><br>
<div id="settingsExit">
	<<settingsExit>>
</div>
<</if>>
<br><br>
<</nobr>><</widget>>

<<widget "settingsExit">><<nobr>>
<<if $passage is "Start" or $passage is "certification_event_start">>
	<<set _buttonName to "시작">>
<<else>>
	<<set _buttonName to "확인">>
<</if>>
<<link _buttonName>>
	<<if ($feats.locked is true or $cheatdisable is "t") and ($feats.soft is true or ($gamemode isnot "soft" and $alluremod gte 1))>>
		<<settingsExitFunction>>
	<<else>>
		<<replace #settingsExit>><<settingsExitConfirm>><</replace>>
	<</if>>
<</link>>
<</nobr>><</widget>>

<<widget "settingsExitConfirm">><<nobr>>
<span class="red">일부 설정은 특정 도전과제를 비활성화시킬 수 있습니다. 괜찮습니까?</span>
<br>
<<link "아뇨, 해당 설정들을 원래대로 되돌려주세요">>
	<<set $cheatdisable to "t">>
	<<if $gamemode is "soft">>
		<<set $gamemode to "normal">>
	<</if>>
	<<if $alluremod lt 1>>
		<<set $alluremod to 1>>
	<</if>>
	<<replace #settingsExit>><<settingsExit>><</replace>>
	<<displaySettings>>
<</link>>
<br><br><br>
<<link "네">>
	<<settingsExitFunction>>
<</link>>
<</nobr>><</widget>>

<<widget "settingsExitFunction">><<nobr>>
	<<if $settingsExitPassage is "Start2">>
		<<setSkinColorBase>>
	<</if>>
	<<if $cheatdisable is "f" and !$debug>>
		<<set $feats.locked to true>>
	<</if>>
	<<if $gamemode is "soft" or $alluremod lt 1>>
		<<set $feats.soft to true>>
	<</if>>
	<<initnpcgender>>
	<<hirsuteHideCheck>>
	<<tanned 0 "ignoreCoverage">>
	<<if $player.freckles is "random">>
		<<set $player.freckles to random(1, 5) == 1 ? true : false>>
	<</if>>
	<!-- check if specified face style is installed -->
	<img id="testImage" class="hidden" @scr="'img/face/' + $facestyle + '/base.png'">
	<<if $facestyle is "" or isImageOk("#testImage")>>
		<<set $facestyle = "default">>
	<</if>>
	<<set _settingsExitPassage to clone($settingsExitPassage)>>
	<<unset $settingsExitPassage>>
	<<run State.display(_settingsExitPassage)>>
	<<set $saveName to $saveName.replace(/[^a-zA-Z 0-9]+/g,"")>>
	<<unset $importString>>
<</nobr>><</widget>>
/*complete:{"Vrelnir (Default)":"vrelDefault","Lollipop Scythe":"lollipopScythe","PurityGuy":"purityGuy","Fangi":"fangi"},*/
<<widget "quickStartOptions">><<nobr>>
<<if _quickStartOptions is undefined>>
	<<set _quickStartOptions to {
		player:{"Female Waif":"femaleWaif","Male Waif":"maleWaif", "Hermaphrodite Waif":"hermaphroditeWaif", "Vrelnir PC":"vrel", "Crossdresser":"crossdresser","Tough Guy":"toughGuy"},
		genericNpc:{"Default Genders":"genericNpcDefaultGender", "All Male":"genericNpcAllMaleGender", "All Female":"genericNpcAllFemaleGender"},
		namedNpc:{"Default Genders":"namedNpcDefaultGender", "All Male":"namedNpcAllMaleGender", "All Female":"namedNpcAllFemaleGender"},
		beasts:{"Default Genders":"beastDefaultGenders", "All Male":"beastMaleGenders", "All Female":"beastFemaleGenders"},
		monsters:{"When Hallucinating":"monstersDefault", "No Monsters":"monstersNone", "All Monsters":"monstersAll"},
		kink:{"Reasonably Debauched":"kinkDefault", "Vanilla":"kinkVanilla", "No Beasts":"kinkNoBeasts", "Everything Goes":"kinkEverything"},
		game:{
			"Normal ":"gameNormal","Soft (No Feats)":"gameSoft","Hard":"gameHard","Masochist":"gameMasochist","Soft with Cheats (No Feats)":"gameSoftCheats","Normal with Cheats (No Feats)":"gameNormalCheats","Hard with Cheats (No Feats)":"gameHardCheats","Masochist with Cheats (No Feats)":"gameMasochistCheats"
		},
		images:{"Default":"imagesdefault","High Quality":"imagesHigh","Low Quality":"imagesLow","Performance Mode":"imagesPerformance","Disable images":"imagesDisable"},
		settings:{"PC, Tips":"settingPCTip","PC, no Tips":"settingPCNoTip","Mobile, Tips":"settingMobileTips","Mobile, no Tips":"settingMobileNoTips"}
	}>>
<</if>>
<</nobr>><</widget>>

<<widget "quickStart">><<nobr>>
<<quickStartOptions>>
<i>Gender ratios and kinks can be fine-tuned in the Game Settings Tab.</i>
<br><br>
/*
<span class="gold">Complete Contributor Preset:</span> <<listbox "_completePreset" autoselect>><<optionsfrom _quickStartOptions.complete>><</listbox>> |
<<link Set>><<presets _completePreset>><</link>>
<br><br>
*/

/*
<span class="gold">Player Preset:</span> <<listbox "_playerPreset" autoselect>><<optionsfrom _quickStartOptions.player>><</listbox>> |
<<link Set>><<presets _playerPreset>><</link>>
<br><br>
*/

<span class="gold">Generic NPC Preset:</span> <<listbox "_genericNpcPreset" autoselect>><<optionsfrom _quickStartOptions.genericNpc>><</listbox>> |
<<link Set>><<presets _genericNpcPreset>><</link>>
<br><br>
<span class="gold">Named NPC Preset:</span> <<listbox "_namedNpcPreset" autoselect>><<optionsfrom _quickStartOptions.namedNpc>><</listbox>> |
<<link Set>><<presets _namedNpcPreset>><</link>>
<br><br>

<span class="gold">Beasts Preset:</span> <<listbox "_beastPreset" autoselect>><<optionsfrom _quickStartOptions.beasts>><</listbox>> |
<<link Set>><<presets _beastPreset>><</link>>
<br><br>
<span class="gold">Beasts as monsters Preset:</span> <<listbox "_monstersPreset" autoselect>><<optionsfrom _quickStartOptions.monsters>><</listbox>> |
<<link Set>><<presets _monstersPreset>><</link>>
<br><br>

<span class="gold">Kinks Preset:</span> <<listbox "_kinkPreset" autoselect>><<optionsfrom _quickStartOptions.kink>><</listbox>> |
<<link Set>><<presets _kinkPreset>><</link>>
<br><br>

<span class="gold">Difficulty Preset:</span> <<listbox "_gamePreset" autoselect>><<optionsfrom _quickStartOptions.game>><</listbox>> |
<<link Set>><<presets _gamePreset>><</link>>
<br><br>
<<if StartConfig.enableImages is true>>
	<span class="gold">Images Preset:</span> <<listbox "_imagesPreset" autoselect>><<optionsfrom _quickStartOptions.images>><</listbox>> |
	<<link Set>><<presets _imagesPreset>><</link>>
	<br><br>
<</if>>
<span class="gold">Settings Preset:</span> <<listbox "_settingsPreset" autoselect>><<optionsfrom _quickStartOptions.settings>><</listbox>> |
<<link Set>><<presets _settingsPreset>><</link>>
<br><br>
<</nobr>><</widget>>

<<widget "characterSettings">><<nobr>>

	<<if StartConfig.enableImages is true>>
		<span class="gold">사이드 바에서 미리보기.</span> <label>겉옷 보기 <<print '<<checkbox "_showClothes" false true '+(_showClothes isnot false ? "checked" : "")+'>>'>></label> <label>속옷 보기 <<print '<<checkbox "_showUnderwear" false true '+(_showUnderwear isnot false ? "checked" : "")+'>>'>></label> 
		<br>
		<span class="red">'플레이어 캐릭터의 피부색 시각적으로 표현' 기능은 일반 탭에서 활성화해야 정상적으로 게임 플레이 중에 피부색을 볼 수 있습니다.</span>
		<br>
	<</if>>
	<br>

	<<if $dev is 1>>
		<span class="gold">난이도</span>
		<br>
		<span class="red">어려움</span> <------------------------------> <span class="green">쉬움</span>
		<br>
		<<numberslider "$devlevel" $devlevel 6 16 1>>
		<br><br>
	<<else>>
		<span class="gold">신체 크기</span> - 몸이 작을 수록 인카운터 중에 가할 수 있는 피해량이 적어져, 저항이 힘들어집니다.
		<br>
		<label>조그마함 <<print '<<radiobutton "$bodysize" 0 ' + ($bodysize is 0 ? "checked" : "") + '>>'>></label> |
		<label>작음 <<print '<<radiobutton "$bodysize" 1 ' + ($bodysize is 1 ? "checked" : "") + '>>'>></label> |
		<label>평범함 <<print '<<radiobutton "$bodysize" 2 ' + ($bodysize is 2 ? "checked" : "") + '>>'>></label> |
		<label>큼 <<print '<<radiobutton "$bodysize" 3 ' + ($bodysize is 3 ? "checked" : "") + '>>'>></label>
		<br><br>
	<</if>>

	<span class="gold">성별</span>
	<br>
	<label>남성 <<print '<<radiobutton "$player.gender" "m" ' + ($player.gender is "m" ? "checked" : "") + '>>'>></label> |
	<label>여성 <<print '<<radiobutton "$player.gender" "f" ' + ($player.gender is "f" ? "checked" : "") + '>>'>></label> |
	<label>후타나리 <<print '<<radiobutton "$player.gender" "h" ' + ($player.gender is "h" ? "checked" : "") + '>>'>></label>
	<br><br>

	<span class="gold">체형</span>
	<br>
	<label>남성적 <<print '<<radiobutton "$player.gender_body" "m" ' + ($player.gender_body is "m" ? "checked" : "") + '>>'>></label> |
	<label>여성적 <<print '<<radiobutton "$player.gender_body" "f" ' + ($player.gender_body is "f" ? "checked" : "") + '>>'>></label> |
	<label>중성적 <<print '<<radiobutton "$player.gender_body" "a" ' + ($player.gender_body is "a" ? "checked" : "") + '>>'>></label> |
	<br><br>

	<<if $dev is 1>>
		<span class="gold">성기</span>
		<br>
		<label>자지 <<print '<<radiobutton "$player.sex" "m" ' + ($player.sex is "m" ? "checked" : "") + '>>'>></label> |
		<label>보지 <<print '<<radiobutton "$player.sex" "f" ' + ($player.sex is "f" ? "checked" : "") + '>>'>></label>
		<br><br>
	<</if>>

	<span class="gold">자지 크기</span> - 적용 가능할 경우
	<br>
	<label>조그마함 <<print '<<radiobutton "$penissize" 0 ' + ($penissize is 0 ? "checked" : "") + '>>'>></label> |
	<label>작음 <<print '<<radiobutton "$penissize" 1 ' + ($penissize is 1 ? "checked" : "") + '>>'>></label> |
	<label>평범함 <<print '<<radiobutton "$penissize" 2 ' + ($penissize is 2 ? "checked" : "") + '>>'>></label>
	<br><br>

	<span class="gold">가슴 크기</span>
	<br>
	<label>평평함 <<print '<<radiobutton "$breastsize" 0 ' + ($breastsize is 0 ? "checked" : "") + '>>'>></label> |
	<label>약간 솟아오름 <<print '<<radiobutton "$breastsize" 1 ' + ($breastsize is 1 ? "checked" : "") + '>>'>></label> |
	<label>조그마함 <<print '<<radiobutton "$breastsize" 2 ' + ($breastsize is 2 ? "checked" : "") + '>>'>></label> |
	<label>작음 <<print '<<radiobutton "$breastsize" 3 ' + ($breastsize is 3 ? "checked" : "") + '>>'>></label> |
	<label>앙증맞음 <<print '<<radiobutton "$breastsize" 4 ' + ($breastsize is 4 ? "checked" : "") + '>>'>></label>
	<br><br>

	<span class="gold">엉덩이 크기</span>
	<br>
	<label>호리호리함 <<print '<<radiobutton "$bottomsize" 0 ' + ($bottomsize is 0 ? "checked" : "") + '>>'>></label> |
	<label>날씬함 <<print '<<radiobutton "$bottomsize" 1 ' + ($bottomsize is 1 ? "checked" : "") + '>>'>></label> |
	<label>평범함 <<print '<<radiobutton "$bottomsize" 2 ' + ($bottomsize is 2 ? "checked" : "") + '>>'>></label> |
	<label>푹신함 <<print '<<radiobutton "$bottomsize" 3 ' + ($bottomsize is 3 ? "checked" : "") + '>>'>></label>
	<br><br>

	<span class="gold">고환</span> - 후타나리인 경우에만 적용
	<br>
	<label>있음 <<print '<<radiobutton "$player.ballsExist" true ' + ($player.ballsExist is true ? "checked" : "") + '>>'>></label> |
	<label>없음 <<print '<<radiobutton "$player.ballsExist" false ' + ($player.ballsExist is false ? "checked" : "") + '>>'>></label> |
	<br><br>

	<span class="gold">가슴 감도</span>
	<br>
	<label>평범함 <<print '<<radiobutton "$breastsensitivity" 1 ' + ($breastsensitivity is 1 ? "checked" : "") + '>>'>></label> |
	<label><span class="pink">민감함</span> <<print '<<radiobutton "$breastsensitivity" 3 ' + ($breastsensitivity is 3 ? "checked" : "") + '>>'>></label> |
	<label><span class="red">아주 민감함</span> <<print '<<radiobutton "$breastsensitivity" 5 ' + ($breastsensitivity is 5 ? "checked" : "") + '>>'>></label>
	<br><br>

	<span class="gold">성기 감도</span>
	<br>
	<label>평범함 <<print '<<radiobutton "$genitalsensitivity" 1 ' + ($genitalsensitivity is 1 ? "checked" : "") + '>>'>></label> |
	<label><span class="pink">민감함</span> <<print '<<radiobutton "$genitalsensitivity" 3 ' + ($genitalsensitivity is 3 ? "checked" : "") + '>>'>></label> |
	<label><span class="red">아주 민감함</span> <<print '<<radiobutton "$genitalsensitivity" 5 ' + ($genitalsensitivity is 5 ? "checked" : "") + '>>'>></label>
	<br><br>

	<span class="gold">눈 색</span>
	<br>
	<label><span class="purple">보라</span> <<print '<<radiobutton "$eyeselect" "purple" ' + ($eyeselect is "purple" ? "checked" : "") + '>>'>></label> |
	<label><span class="blue">짙은 파랑</span> <<print '<<radiobutton "$eyeselect" "dark blue" ' + ($eyeselect is "dark blue" ? "checked" : "") + '>>'>></label> |
	<label><span class="lblue">밝은 파랑</span> <<print '<<radiobutton "$eyeselect" "light blue" ' + ($eyeselect is "light blue" ? "checked" : "") + '>>'>></label> |
	<label><span class="tangerine">호박색</span> <<print '<<radiobutton "$eyeselect" "amber" ' + ($eyeselect is "amber" ? "checked" : "") + '>>'>></label> |
	<label><span class="brown">적갈색</span> <<print '<<radiobutton "$eyeselect" "hazel" ' + ($eyeselect is "hazel" ? "checked" : "") + '>>'>></label> |
	<label><span class="green">초록</span> <<print '<<radiobutton "$eyeselect" "green" ' + ($eyeselect is "green" ? "checked" : "") + '>>'>></label> |
	<label><span class="red">빨강</span> <<print '<<radiobutton "$eyeselect" "red" ' + ($eyeselect is "red" ? "checked" : "") + '>>'>></label> |
	<label><span class="pink">분홍</span> <<print '<<radiobutton "$eyeselect" "pink" ' + ($eyeselect is "pink" ? "checked" : "") + '>>'>></label> |
	<label><span class="grey">회색</span> <<print '<<radiobutton "$eyeselect" "grey" ' + ($eyeselect is "grey" ? "checked" : "") + '>>'>></label>
	<br><br>

	<span class="gold">머리카락 색</span>
	<br>
	<label><span class="red">빨강</span> <<print '<<radiobutton "$hairselect" "red" ' + ($hairselect is "red" ? "checked" : "") + '>>'>></label> |
	<label><span class="black">Jet Black</span> <<print '<<radiobutton "$hairselect" "jetblack" ' + ($hairselect is "jetblack" ? "checked" : "") + '>>'>></label> |
	<label><span class="black">검정</span> <<print '<<radiobutton "$hairselect" "black" ' + ($hairselect is "black" ? "checked" : "") + '>>'>></label> |
	<label><span class="brown">갈색</span> <<print '<<radiobutton "$hairselect" "brown" ' + ($hairselect is "brown" ? "checked" : "") + '>>'>></label> |
