	<</if>>
<</widget>>

<<widget "bathroomLink">>
	<<if getRobinLocation() is "bath" and $halloween isnot 1>>
		<<if $robinBathKnown>>
			Robin is in the bath.
			<br>
		<</if>>
		<<bathroomicon>><<link [[욕실 (0:01)|Robin Bath]]>><<pass 1>><</link>>
	<<else>>
		<<if $robinBathKnown>>
			<<unset $robinBathKnown>>
		<</if>>
		<<bathroomicon>><<link [[욕실 (0:01)|Bathroom]]>><<pass 1>><</link>>
	<</if>>
<</widget>>
